{
  "extends": ["airbnb/hooks", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi": 0,
    "react/prop-types": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}

/**
 * React/Eslint rules
 * https://github.com/yannickcr/eslint-plugin-react#recommended
 */
